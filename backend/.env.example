# Configurações do Bling
BLING_CLIENT_ID=your_bling_client_id_here
BLING_CLIENT_SECRET=your_bling_client_secret_here
BLING_REDIRECT_URI=http://localhost:3000/auth/bling/callback
BLING_SCOPE=profile,read,write

# URLs da API do Bling
BLING_AUTH_URL=https://bling.com.br/Api/v3/oauth/authorize
BLING_TOKEN_URL=https://bling.com.br/Api/v3/oauth/token
BLING_API_BASE_URL=https://bling.com.br/Api/v3

# Configurações do servidor
PORT=3000
NODE_ENV=development

# JWT Secret
JWT_SECRET=your_super_secure_jwt_secret_here

# Base URL da aplicação
BASE_URL=http://localhost:3000

# Frontend URL
FRONTEND_URL=http://localhost:3001
